{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "react-router",
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "outputDirectory": "build/client",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/__react-router"
    }
  ]
}
