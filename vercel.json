{
  "version": 2,
  "functions": {
    "api/ssr.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/build/client/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/build/client/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/api/ssr.js"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build"
}
